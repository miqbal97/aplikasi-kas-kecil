	<queryString>
		<![CDATA[SELECT
     pengeluaran_dana.`tanggal` AS tanggal,
     pengeluaran_dana.`no_rekening` AS rekening,
     pengeluaran_dana.`keterangan` AS keterangan,
     pengeluaran_dana.`nominal` AS nominal,
     pengeluaran_dana.`uraian` AS uraian
FROM
     `pengeluaran_dana` pengeluaran_dana
